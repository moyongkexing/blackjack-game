{
  "compilerOptions": {
    "target": "es5",
    "module": "umd",
    "lib": [
      "ES5",
      "ES2016",
      "DOM",
      "ScriptHost"],
    "outDir": "./js",             
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
